<div id="container-index">
  <div class="row">
    <h1>Stores in your area</h1>
    <div class="small-6 large-9 columns">
      <div id="map"></div>
    </div>
    <div class="small-6 large-3 columns">
      <%= render "stores/aside" %>
    </div>  
  </div>
  <div class="row">
    <div class="small-6 large-9 columns">
      <ul class="small-block-grid-2 medium-block-grid-3 large-block-grid-4">
        <%= render partial: "store_element" %>
      </ul>
    </div>
    <div class="small-6 large-3 columns">
  </div>
</div>

<script async defer src="https://maps.googleapis.com/maps/api/js?callback=initialise"></script>







